{
  "name": "possible",
  "version": "0.0.9",
  "description": "todo list",
  "main": "./out/main/index.js",
  "author": "guojian_k@qq.com",
  "homepage": "https://github.com/padeyao4/possible",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -w",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "pnpm run build && electron-builder --win --config",
    "build:mac": "pnpm run build && electron-builder --mac --config",
    "build:linux": "pnpm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@antv/dom-util": "^2.0.4",
    "@antv/g-base": "^0.5.15",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@prisma/client": "^5.3.1",
    "electron-updater": "^6.1.1",
    "lodash": "^4.17.21",
    "pinia": "^2.1.6",
    "uuid": "^9.0.0",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antv/g6": "^4.8.21",
    "@antv/g6-core": "^0.8.21",
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/node": "^18.17.5",
    "@types/uuid": "^9.0.3",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "element-plus": "^2.3.12",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "less": "^4.2.0",
    "prettier": "^3.0.2",
    "prisma": "^5.3.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  }
}
