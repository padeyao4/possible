{
  "name": "possible",
  "version": "v0.0.49",
  "description": "todo list",
  "main": "./out/main/index.js",
  "author": "guojian_k@qq.com",
  "homepage": "https://github.com/padeyao4/possible",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -w",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "pnpm run build && electron-builder --win --config",
    "build:mac": "pnpm run build && electron-builder --mac --config",
    "build:linux": "pnpm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@antv/g6": "^5.0.0-beta.28",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@icon-park/vue-next": "^1.4.2",
    "class-transformer": "^0.5.1",
    "default-passive-events": "^2.0.0",
    "electron-log": "^5.0.3",
    "electron-updater": "^6.1.7",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "reflect-metadata": "^0.2.1",
    "uuid": "^9.0.1",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "default-passive-events": "^2.0.0",
    "electron": "^28.1.1",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0-beta.3",
    "element-plus": "^2.4.4",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "less": "^4.2.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.1",
    "vue": "^3.4.4",
    "vue-tsc": "^1.8.27"
  }
}
