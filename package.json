{
  "name": "possible",
  "version": "0.0.10",
  "description": "todo list",
  "main": "./out/main/index.js",
  "author": "guojian_k@qq.com",
  "homepage": "https://github.com/padeyao4/possible",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -w",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "pnpm run build && electron-builder --win --config",
    "build:mac": "pnpm run build && electron-builder --mac --config",
    "build:linux": "pnpm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@antv/dom-util": "^2.0.4",
    "@antv/g-base": "^0.5.15",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.1",
    "@icon-park/vue-next": "^1.4.2",
    "default-passive-events": "^2.0.0",
    "electron-updater": "^6.1.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "uuid": "^9.0.1",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antv/g6": "^4.8.23",
    "@antv/g6-core": "^0.8.23",
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/node": "^18.18.8",
    "@types/uuid": "^9.0.6",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "default-passive-events": "^2.0.0",
    "electron": "^25.9.3",
    "electron-builder": "^24.6.4",
    "electron-vite": "^1.0.28",
    "element-plus": "^2.4.2",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "less": "^4.2.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue": "^3.3.7",
    "vue-tsc": "^1.8.22"
  }
}
