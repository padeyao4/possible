{
  "files": [],
  "include": [
    "electron.vite.config.*",
    "src/main/*",
    "src/preload/*",
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.vue",
    "src/renderer/src/**/*.json",
    "src/preload/*.d.ts"
  ],
  "exclude": [
    "@electron-toolkit/tsconfig/tsconfig.node.json",
    "@electron-toolkit/tsconfig/tsconfig.web.json"
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "electron-vite/node"
    ],
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    }
  }
}
