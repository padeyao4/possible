{
  "files": [],
  "include": [
    "electron.vite.config.*",
    "src/main/*",
    "src/preload/*",
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.vue",
    "src/renderer/src/**/*.json",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "downlevelIteration": true,
    "types": [
      "electron-vite/node",
      "src/preload/index.d.ts"
    ],
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
